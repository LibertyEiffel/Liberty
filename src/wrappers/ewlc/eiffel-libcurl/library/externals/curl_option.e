			-- 
			-- CURLFTPSSL_CONTROL
			-- 
			-- Require SSL for the control connection or fail with CURLE_FTP_SSL_FAILED.
			-- 
			-- CURLFTPSSL_ALL
			-- 
			-- Require SSL for all communication or fail with CURLE_FTP_SSL_FAILED.
			-- 
			-- CURLOPT_FTPSSLAUTH
