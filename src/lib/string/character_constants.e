expanded class CHARACTER_CONSTANTS
   --
   -- Platform-independent, universal, character constants.
   -- Intended to be used as ancestor for classes that need these constants.
   --

feature {ANY} -- Character names:
   Ctrl_a: CHARACTER '%/1/'

   Ctrl_b: CHARACTER '%/2/'

   Ctrl_c: CHARACTER '%/3/'

   Ctrl_d: CHARACTER '%/4/'

   Ctrl_e: CHARACTER '%/5/'

   Ctrl_f: CHARACTER '%/6/'

   Ctrl_g: CHARACTER '%/7/'

   Ch_bs: CHARACTER '%/8/'

   Ch_tab: CHARACTER '%/9/'

   Ctrl_j: CHARACTER '%/10/'

   Ctrl_k: CHARACTER '%/11/'

   Ctrl_l: CHARACTER '%/12/'

   Ctrl_m: CHARACTER '%/13/'

   Ctrl_n: CHARACTER '%/14/'

   Ctrl_o: CHARACTER '%/15/'

   Ctrl_p: CHARACTER '%/16/'

   Ctrl_q: CHARACTER '%/17/'

   Ctrl_r: CHARACTER '%/18/'

   Ctrl_s: CHARACTER '%/19/'

   Ctrl_t: CHARACTER '%/20/'

   Ctrl_u: CHARACTER '%/21/'

   Ctrl_v: CHARACTER '%/22/'

   Ctrl_w: CHARACTER '%/23/'

   Ctrl_x: CHARACTER '%/24/'

   Ctrl_y: CHARACTER '%/25/'

   Ctrl_z: CHARACTER '%/26/'

   Ch_del: CHARACTER '%/127/'

end -- class CHARACTER_CONSTANTS
